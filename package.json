{
  "name": "archive-it",
  "version": "1.0.0",
  "description": "A utility to archive a Replit User's projects",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write",
    "clean:tmp": "rimraf tmp/**",
    "clean:projects": "rimraf projects/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RayhanADev/ArchiveIt.git"
  },
  "keywords": [
    "Replit",
    "ReplAPIit",
    "replapi-it",
    "archive",
    "zip"
  ],
  "author": "RayhanADev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RayhanADev/ArchiveIt/issues"
  },
  "homepage": "https://github.com/RayhanADev/ArchiveIt#readme",
  "dependencies": {
    "adm-zip": "^0.5.5",
    "replapi-it": "^2.1.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1"
  }
}
